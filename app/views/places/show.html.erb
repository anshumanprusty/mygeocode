<%#=raw @places.to_json %>

<table border="1">
	<tr>
		<td>ID</td>
		<td>Place</td>
		<td>Longitude</td>
		<td>Latitude</td>
		<td>Visited By</td>
	</tr>
  <% @places.each do |event| %>
  <tr>
      <% event.each do |e| %>
          <td><%= e[1] %></td>
      <% end %>
  </tr>
  <% end %>
</table>

